{
  "name": "ccnew",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "concurrently -k \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "cross-env TS_NODE_PROJECT=tsconfig.server.json tsx watch server/app.ts",
    "dev:client": "cross-env ELECTRON=true NODE_ENV=development vite --config client/vite.config.ts",
    "dev:debug": "cross-env NODE_OPTIONS=\"--inspect=9229\" pnpm run dev",
    "type-check": "tsc -p client/tsconfig.json && tsc -p server/tsconfig.server.json",
    "ts-report": "pnpm run type-check --pretty false --noEmit > ts-report.log",
    "log-check": "node scripts/analyse-log.js",
    "build": "vite build --config client/vite.config.ts"
  },
