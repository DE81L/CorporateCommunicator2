    "dev:server": "cross-env DB_DISABLED=true ts-node-dev --respawn --transpile-only --project tsconfig.server.json server/app.ts",
    "dev:client": "cd client && cross-env ELECTRON=true NODE_ENV=development vite",
    "dev": "concurrently -k \"npm:dev:server\" \"npm:dev:client\"",
    "debug:vite": "vite dev --debug",
    "build": "tsc -p tsconfig.server.json && cd client && vite build",
    "check": "tsc -p tsconfig.json --noEmit",
    "dev:debug": "cross-env LOG_LEVEL=debug npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev:nodb": "cross-env DB_DISABLED=true npm run dev"
