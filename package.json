  "scripts": {
    "dev:api": "npx tsx watch server/index.ts",
    "dev": "concurrently -k \"npm:dev:api\" \"wait-on tcp:3000 && cd client && cross-env ELECTRON=true vite\" \"npm:dev:electron\"",
    "dev:autoauth": "AUTO_AUTH=true npx tsx watch server/index.ts",
    "dev:electron": "wait-on http://localhost:5173 && cross-env NODE_ENV=development ELECTRON=true electron electron/main.cjs --no-sandbox",
    "check:db": "tsx scripts/check-db.ts",
    "dev:client": "cd client && cross-env ELECTRON=true NODE_ENV=development vite",
    "debug:api": "tsx scripts/debug-api.ts",
    "build": "cross-env ELECTRON=true NODE_ENV=production vite build && npm run build:electron",
    "check:db:user": "tsx scripts/check-db-and-user.ts",
    "build:electron": "tsc -p electron/tsconfig.json",
    "start": "cross-env NODE_ENV=production ELECTRON=true electron .",
    "check": "tsc -p client/tsconfig.json"
  },
